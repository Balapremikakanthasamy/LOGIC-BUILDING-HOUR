#include<stdio.h>
#include<string.h>
int NthPrime(int input1)
{
  int count=0;
  int pcount=0;
  int i;
  for(i=2;i>0;i++){
    pcount=0;
    for(int j=2;j<i;j++){
        if(i%j==0){
            pcount=1;
        break;
         }
  }
  if(pcount==0)
    count++;
  if(count==input1)
    return 1;
  }
  return 0;
}
